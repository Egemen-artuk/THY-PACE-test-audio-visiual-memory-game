{
  "headers": [
    {
      "source": "/audio/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    },
    {
      "source": "**/*.opus",
      "headers": [
        {
          "key": "Content-Type",
          "value": "audio/opus"
        }
      ]
    }
  ]
}
